            </ul> 
          </div>
        </nav>

         <div class="col s12">
          <center>
          <br>
          <br>
          <a class="main-green-text crete-round"><font size="6">Tutee Profile</font></a>          
          <br>
          <img src="images/line2.png" height="55">
          <br>
          </center>
        </div>
<%= simple_form_for @tutee do |f| %>
 <div class="row">
  <div class="container">
    <form class="col s12">

      <div class="row">
        <div class="input-field col s3">
          <%= f.input :first_name %>
        </div>
        <div class="input-field col s3">
          <%= f.input :last_name %>
        </div>
        <div class="input-field col s3">
          <%= f.input :father_name %>
        </div>
        <div class="input-field col s3">
          <%= f.input :father_occupation %>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s6">
          <%= f.input :school %>
        </div>
          
          <div class="input-field col s3">
          <%= f.input :mother_name %>
        </div>
        <div class="input-field col s3">
          <%= f.input :mother_occupation %>
        </div>
      </div>

      <div class="row">
        <div class="input-field col s3">
<%= f.input :group_no %>
    </div>
        <div class="input-field col s6 offset-s3">
	<%= f.input :ambition %>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s3">
          	<%= f.input :math_grade %>
        </div>
        <div class="input-field col s3">
          <%= f.input :eng_grade %>
        </div>
        <div class="input-field col s3 ">
          <%= f.input :fav_subject %>
        </div>
        <div class="input-field col s3">
          <%= f.input :fav_color %>
        </div>
      </div>

      <div class="row">
      </div>
      
      <div class="row">
                  <div class="col s12 offset-s8">
                      	<span><%= f.submit :Save, class: 'btn btn-main-green' %></span>
                    <div class="btn btn-main-green">
                      <span>Cancel</span>
                    </div> 
                  </div>                 
                </div>  
 <% end %>     


      
    </form>
    </div>
  </div>
    </body>
  </html>